public enum AccountInfoStatusEnum {

    /**
     * 注销
     */
    CANCEL("0"),
    /**
     * 正常
     */
    NORMAL("1"),
    /**
     * 冻结
     */
    FROZEN("2"),

    /**
     * 卡绑定
     */
    BINDED("3"),

    /**
     * 未激活
     */
    UNACTIVATED("5"),
    /**
     * 止付
     */
    STOP_PAYMENT("6"),
    /**
     * 挂起或暂停
     */
    SUSPEND("7");

    private String value;

    AccountInfoStatusEnum(String value) {
        this.value = value;
    }

    public String value() {
        return value;
    }
}
