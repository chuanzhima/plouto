package com.smk.pay.core.dto;

import java.util.Date;

/**
 * Project plouto
 * Created by chuanzhi.macz
 * Date 2016/11/10 19:26
 */
public class MerchantAccountInfoDto {

    public static final String MERCHANT_ACCOUNT_TYPE_1 = "1";

    public static final String MERCHANT_ACCOUNT_STATUS_1 = "1";

    private String merAccountId;

    private String merchantId;

    private String merchantName;

    private String accountType;

    private String status;

    private String createSerialNo;

    private Date createDate;

    private Date lastUpdateDate;

    private String partyMarkNo;

    public MerchantAccountInfoDto() {
        super();
    }

    public MerchantAccountInfoDto(String merchantId) {
        this.merchantId = merchantId;
    }

    public MerchantAccountInfoDto(String merAccountId, String merchantId, String merchantName, String accountType, String status, String createSerialNo, Date createDate, Date lastUpdateDate, String partyMarkNo) {
        this.merAccountId = merAccountId;
        this.merchantId = merchantId;
        this.merchantName = merchantName;
        this.accountType = accountType;
        this.status = status;
        this.createSerialNo = createSerialNo;
        this.createDate = createDate;
        this.lastUpdateDate = lastUpdateDate;
        this.partyMarkNo = partyMarkNo;
    }

    public String getMerAccountId() {
        return merAccountId;
    }

    public void setMerAccountId(String merAccountId) {
        this.merAccountId = merAccountId;
    }

    public String getMerchantId() {
        return merchantId;
    }

    public void setMerchantId(String merchantId) {
        this.merchantId = merchantId;
    }

    public String getMerchantName() {
        return merchantName;
    }

    public void setMerchantName(String merchantName) {
        this.merchantName = merchantName;
    }

    public String getAccountType() {
        return accountType;
    }

    public void setAccountType(String accountType) {
        this.accountType = accountType;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getCreateSerialNo() {
        return createSerialNo;
    }

    public void setCreateSerialNo(String createSerialNo) {
        this.createSerialNo = createSerialNo;
    }

    public Date getCreateDate() {
        return createDate;
    }

    public void setCreateDate(Date createDate) {
        this.createDate = createDate;
    }

    public Date getLastUpdateDate() {
        return lastUpdateDate;
    }

    public void setLastUpdateDate(Date lastUpdateDate) {
        this.lastUpdateDate = lastUpdateDate;
    }

    public String getPartyMarkNo() {
        return partyMarkNo;
    }

    public void setPartyMarkNo(String partyMarkNo) {
        this.partyMarkNo = partyMarkNo;
    }

    @Override
    public String toString() {
        final StringBuilder sb = new StringBuilder("MerchantAccountInfoDto{");
        sb.append("merAccountId='").append(merAccountId).append('\'');
        sb.append(", merchantId='").append(merchantId).append('\'');
        sb.append(", merchantName='").append(merchantName).append('\'');
        sb.append(", accountType='").append(accountType).append('\'');
        sb.append(", status='").append(status).append('\'');
        sb.append(", createSerialNo='").append(createSerialNo).append('\'');
        sb.append(", createDate=").append(createDate);
        sb.append(", lastUpdateDate=").append(lastUpdateDate);
        sb.append(", partyMarkNo='").append(partyMarkNo).append('\'');
        sb.append('}');
        return sb.toString();
    }
}
