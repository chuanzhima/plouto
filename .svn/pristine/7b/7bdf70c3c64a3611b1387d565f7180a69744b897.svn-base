public class ServiceResultResponse extends RpcResponse {

    public ServiceResultResponse() {
        super();
    }

    public ServiceResultResponse(Boolean isSucc) {
        super(isSucc);
    }

    public ServiceResultResponse(Serializable serializable, Boolean isSucc) {
        super(serializable, isSucc);
    }

    public ServiceResultResponse(Boolean isSucc, String resultCode, String resultMsg) {
        super(isSucc, resultCode, resultMsg);
    }
}
